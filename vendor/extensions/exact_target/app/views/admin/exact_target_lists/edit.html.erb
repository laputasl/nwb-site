<%= render :partial => 'admin/shared/configuration_menu' %>

<h1><%= t("exact_target.editing") %></h1>
<%= error_messages_for :exact_target_list %>
<% form_for(@exact_target_list, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag t("update")%>
  </p>
<% end %>