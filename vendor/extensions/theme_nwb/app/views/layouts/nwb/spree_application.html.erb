<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<%= render :partial => 'shared/head'%>
</head>
  
<!-- gwo control -->
<% gwo_test = GwoTest.find_by_name("NWB Product Headline Color Test MV") %>
<% if ((not gwo_test.nil?) && gwo_test.status == "enabled") %>
    <%= render :partial => 'shared/gwo/nwb_control', :locals => {:eid => "#{gwo_test.eid}"} %>
<% end %>

<% if ((not gwo_test.nil?) && gwo_test.status == "enabled") %>
  <script>utmx_section('body_tag')</script>
<% end %>
<body>
<% if ((not gwo_test.nil?) && gwo_test.status == "enabled") %>
  <%= render :text=>"</noscript>" %>
<% end %>

  <div id="container">
    <%= render "/shared/nwb_navigation" %>
    <div id="main">
      <% if flash[:notice] %>
        <div class="flash notice"><%= flash[:notice] %></div>
      <% end %>
      <% if flash[:error] %>
        <div class="flash errors"><%= flash[:error] %></div>
      <% end %>
      <%= render 'shared/nwb_sidebar' %>
      <div id="body">
        <%= yield :breadcrumbs %>
        <%= yield %>
      </div>
    </div>
    <hr />
    <%= render :partial => "shared/nwb_footer" %>
  </div>
  <%= render 'shared/google_analytics' %>
  <%= render 'shared/nwb_fetch_back' %>
</body>
