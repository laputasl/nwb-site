<% gwo_test = GwoTest.find(gwo_test_id) %>
<% if gwo_test.status=="disabled" %>
  <%= link_to_remote "Enable", :url=>{:action=>"enable", :id=>gwo_test.id}, :method=>:get, :before=>"" %> |
<% else %>
  <%= link_to_remote "Disable", :url=>{:action=>"disable", :id=>gwo_test.id}, :method=>:get, :before=>"" %> |
<% end %>
<%=link_to t("edit"), edit_object_url(gwo_test) %> |
<%=link_to t("delete"), object_url(gwo_test), :method => :delete, :confirm=>"Are you sure to delete?" %>
